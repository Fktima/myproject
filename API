  version: "1.0.0"
  title: "Genome Browser"

host: "notdefined.yet"
basePath: "/v2"
schemes:
- "https"
- "http"
paths:
  /entry:
    get:
      summary: "Finds information on gene"
      description: " Returns information based on accession code, protein product name, chromosomal location or gene identifier"
      parameters:
      - name: accessionCode
        in: query
        required: false
        type: string
        description: "The unque identifier for an entry"
      - name: chromosomalLocation
        in: query
        required: false
        type: string
        description: "The chromosomal ocation used to identify an entry"
      - name: gene
        in: query
        required: false
        type: string
        description: "The gene identifier for an entry"
        
